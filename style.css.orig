  color: #444;
  text-align: center;
}

span {
  padding: 0 0 2px;
  cursor: pointer;
  
  -webkit-transition: .5s;
		 -moz-transition: .5s;
			 -o-transition: .5s;
			-ms-transition: .5s;
			  	transition: .5s;
}

span:hover {
  color: #fd0;
}

.key-wrapper {
  text-align: center;
}

.row {text-align: center;}

.key {
  margin: 3px 1px;
  width: 50px;
  height: 50px;
  border: 2px solid #333;
  border-radius: 5px;
  display: inline-block;

  line-height: 52px;
  text-align: center;
  text-transform: uppercase;
  color: white;
  
 	-webkit-transition: .5s;
		 -moz-transition: .5s;
			 -o-transition: .5s;
			-ms-transition: .5s;
			  	transition: .5s;
}

.k32 {
  width: 346px;
}

.active {
  border: 2px solid white;
  color: #fd0;
  
  -webkit-transition: 0s;
		 -moz-transition: 0s;
			-ms-transition: 0s;
			  	transition: 0s;
}


audio {
  visibility: hidden;
}

button{
  background: transparent;
  border: none;
  margin: 3px 1px;
    }

     .move{
              text-shadow: 0 1px 3px rgba(0, 0, 0, 0.41);
      border:3px solid transparent;
      
    
      animation: pulse 1s linear infinite alternate;
      -webkit-animation: pulse 1s linear infinite alternate;
}

  @-webkit-keyframes pulse {
      100% {
        transform: translateY(6.9px); 
      } 
    }

    @keyframes pulse {
      100% {
        transform: translateY(6.9px); 
      } 
    }
button:focus{
  outline: 0;
}

button:focus .key{
  border: 2px solid #fd0;
  color: #fd0;
  
  -webkit-transition: 0s;
  -moz-transition: 0s;
  -ms-transition: 0s;
  transition: 0s;

}

